import java.util.*;
class Student
{
int admno;
String name;
 void get(int x,String y)
 {
     admno=x;
     name=y;
 }
 void show()
 {
     System.out.println(admno+" "+name);
 }
public static void main(String args[])
{
Student s1=new Student();
s1.get(102,"Reena");
s1.show();
Student s2=new Student();
s2.get(103,"Indu");
s2.show();
}
}
output:
102 Reena
103 Indu
