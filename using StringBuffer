import java.util.*;
class Trim
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
StringBuffer str=new StringBuffer("Java");
StringBuffer str1=new StringBuffer("Language");
System.out.println(str);
System.out.println(str1);
str.append(str1);
System.out.println(str);
}
}
output:
Java
Language
JavaLanguage
